{
  "name": "@conductor/api",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/types.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc && bun build src/index.ts --outdir dist --target bun --minify",
    "check": "biome check --write . && tsc --noEmit",
    "dev": "bun --watch src/index.ts",
    "start": "NODE_ENV=production bun run dist/index.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.858.0",
    "@aws-sdk/s3-request-presigner": "^3.858.0",
    "@conductor/auth": "workspace:*",
    "@conductor/database": "workspace:*",
    "@hono/trpc-server": "^0.4.0",
    "@t3-oss/env-core": "^0.13.8",
    "@trpc/server": "^11.4.3",
    "better-auth": "^1.3.4",
    "hono": "^4.8.9",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "superjson": "^2.2.2",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.2",
    "@types/bun": "^1.2.19",
    "typescript": "^5.8.3"
  },
  "type": "module"
}
